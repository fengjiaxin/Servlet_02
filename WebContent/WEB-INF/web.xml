<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">

  <servlet>
    <servlet-name>welcome</servlet-name>
    <servlet-class>org.feng.servlet.HelloServlet</servlet-class>
    <init-param>
      <param-name>greeting</param-name>
      <param-value>Welcome you you</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>welcome</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>output</servlet-name>
  	<servlet-class>org.feng.servlet.OutputInfoServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>output</servlet-name>
  	<url-pattern>/info</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<servlet-class>org.feng.servlet.LoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>count</servlet-name>
  	<servlet-class>org.feng.servlet.CountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>count</servlet-name>
  	<url-pattern>/count</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>center</servlet-name>
  	<servlet-class>org.feng.servlet.CenterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>center</servlet-name>
  	<url-pattern>/center</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>login2</servlet-name>
  	<servlet-class>org.feng.servlet.LoginServlet2</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>login2</servlet-name>
  	<url-pattern>/login2</url-pattern>
  </servlet-mapping>
</web-app>